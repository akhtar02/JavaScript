        function calcResult(totalObtainedMarks, totalMarks) {
            let perc = (totalObtainedMarks / totalMarks) * 100;
            if (perc > 80) {
                return "A+";
            } else {
                return "B";
            }
        }
        let result = calcResult(200, 600);
        console.log(result);

